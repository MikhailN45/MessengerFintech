<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="Emoji">
        <attr name="reactionCustomBackground" format="reference" />
        <attr name="customNum" format="integer" />
        <attr name="textColor" format="color" />
        <attr name="textSize" format="dimension" />
        <attr name="smiles" format="enum">
            <enum name="smiling" value="1" />
            <enum name="winking" value="2" />
            <enum name="heart" value="3" />
            <enum name="sad" value="4" />
            <enum name="joy" value="5" />
            <enum name="not_interested" value="6" />
            <enum name="hug" value="7" />
            <enum name="party" value="8" />
            <enum name="cry" value="9" />
        </attr>
    </declare-styleable>

    <declare-styleable name="FlexBoxLayout">
        <attr name="paddingColumns" format="dimension"/>
        <attr name="paddingRows" format="dimension"/>
    </declare-styleable>
</resources>
